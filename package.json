{
  "name": "line-liff-challenge",
  "description": "LINE LIFF App",
  "version": "1.0.0",
  "engines": {
    "node": "10.17.0"
  },
  "license": "UNLICENSED",
  "scripts": {
    "dev": "nodemon --exec babel-node --presets @babel/preset-env server/index.js --watch",
    "build": "next build && babel server/ -D -d .next/dist && babel utils/ -D -d .next/utils && cp package.json .next/.",
    "start": "node .next/dist/index.js",
    "test": "jest __tests__ --coverage --forceExit --detectOpenHandles",
    "test:watch": "jest __tests__ --watch --detectOpenHandles",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "db:model:generate": "babel-node node_modules/.bin/sequelize model:generate --name",
    "db:migrate:generate": "babel-node node_modules/.bin/sequelize migration:generate --name",
    "db:migrate": "babel-node node_modules/.bin/sequelize db:migrate",
    "db:migrate:undo": "babel-node node_modules/.bin/sequelize db:migrate:undo",
    "db:seed:generate": "babel-node node_modules/.bin/sequelize seed:generate --name",
    "db:seed": "babel-node node_modules/.bin/sequelize db:seed --seed",
    "db:seed:undo": "babel-node node_modules/.bin/sequelize db:seed:undo --seed",
    "db:seed:undo:all": "babel-node node_modules/.bin/sequelize db:seed:undo:all",
    "db:seed:all": "babel-node node_modules/.bin/sequelize db:seed:all"
  },
  "dependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "7.1.2",
    "@babel/node": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "7.4.2",
    "@babel/preset-react": "7.0.0",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "1.0.1",
    "antd": "^3.13.6",
    "axios": "^0.19.0",
    "axios-auth-refresh": "^1.0.7",
    "babel-loader": "8.0.4",
    "babel-plugin-import": "1.10.0",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-preset-react": "6.24.1",
    "body-parser": "^1.19.0",
    "dotenv": "7.0.0",
    "express": "4.16.4",
    "express-validation": "^1.0.3",
    "isomorphic-unfetch": "3.0.0",
    "joi": "^14.3.1",
    "js-cookie": "^2.2.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "less-vars-to-js": "^1.3.0",
    "lodash": "^4.17.15",
    "memory-cache": "^0.2.0",
    "multer": "^1.4.2",
    "next": "^9.1.1",
    "next-i18next": "^0.52.1",
    "node-sass": "4.13.1",
    "nodemon": "1.18.10",
    "null-loader": "^3.0.0",
    "pg": "^7.14.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-google-recaptcha": "^1.0.5",
    "sequelize": "^5.21.3",
    "swagger-jsdoc": "^3.5.0",
    "swagger-ui-express": "^4.1.2",
    "webpack": "^4.35.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "babel-jest": "24.1.0",
    "eslint": "5.8.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-react-app": "3.0.7",
    "eslint-import-resolver-babel-module": "^5.0.1",
    "eslint-plugin-flowtype": "^3.6.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "21.26.2",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-simple-import-sort": "^4.0.0",
    "jest": "24.1.0",
    "luxon": "^1.21.3",
    "sequelize-cli": "^5.5.1"
  }
}
